<h1>New content</h1>

<% form_for @content, :html => { :multipart => true } do |form| %>
  <%= form.error_messages %>

  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :file %>
    <%= form.file_field :file %>
  </p>

  <p>
    <%= form.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contents_path %>